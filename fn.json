{
    "name": "Fn",
    "scopeName": "source.fn",
    "fileTypes": [
        "fn"
    ],
    "patterns": [
        {
            "name": "comment.line.fn",
            "match": "#.*"
        },
        {
            "name": "constant.numeric.fn",
            "match": "-?[0-9]+(\\.[0-9]+)?"
        },
        {
            "name": "string.quoted.double.fn",
            "match": "\"[\\\"|^\"]*\"?"
        },
        {
            "comment": "Builtins",
            "match": "import!?",
            "name": "keyword.other.fn"
        },
        {
            "name": "entity.name.fn",
            "match": "[A-Za-z0-9_]+[?!]*:"
        },
        {
            "name": "keyword.operator.fn",
            "match": "[@%^&*\\-+=|\\\\<>]{1,2}"
        },
        {
            "comment": "Identifiers",
            "match": "[A-Za-z0-9_]+[?!]*",
            "name": "variable.fn"
        }
    ]
}
